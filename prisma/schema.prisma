datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL") // e.g. mysql://user:password@host:port/dbname
}

generator client {
  provider = "prisma-client-js"
}

model Referral {
  id             Int      @id @default(autoincrement())
  referrerName   String
  referrerEmail  String
  friendName     String
  friendEmail    String
  course         String
  message        String?
  createdAt      DateTime @default(now())
}
